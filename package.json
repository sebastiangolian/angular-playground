{
  "name": "angular-playground",
  "version": "1.0.0",
  "browserslist": [
    "> 5%"
  ],
  "scripts": {
    "start": "ng serve --open --port=4202",
    "start:hmr": "ng serve --hmr --open --port=4202",
    "build": "ng build",
    "build-prod": "ng build --configuration=production",
    "test": "ng test",
    "test-headless": "ng test --browsers ChromeHeadless",
    "eslint:fix": "eslint src/{app,environments,assets}/**/*{.ts,.js} --fix",
    "prettier:write": "prettier --write --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.css,.scss,.html}\"",
    "predeploy": "ng build --configuration production --base-href=/angular-playground/ --outputPath=dist/angular-playground",
    "deploy": "gh-pages -d dist/angular-playground/",
    "clear": "rmdir /q /s node_modules",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
  "lint-staged": {
    "*": [
      "npm run prettier:write"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.7",
    "@angular/common": "^14.2.7",
    "@angular/compiler": "^14.2.7",
    "@angular/core": "^14.2.7",
    "@angular/forms": "^14.2.7",
    "@angular/platform-browser": "^14.2.7",
    "@angular/platform-browser-dynamic": "^14.2.7",
    "@angular/router": "^14.2.7",
    "@types/file-saver": "^2.0.5",
    "@types/leaflet": "^1.5.19",
    "bootstrap": "^4.6.0",
    "file-saver": "^2.0.5",
    "leaflet": "^1.7.1",
    "ngx-bootstrap": "^6.2.0",
    "ngx-filesaver": "^11.0.0",
    "ngx-mask": "^11.1.5",
    "rxjs": "~7.5.7",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.6",
    "@angular-eslint/builder": "^14.1.2",
    "@angular-eslint/eslint-plugin": "^14.1.2",
    "@angular-eslint/eslint-plugin-template": "^14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "^14.1.2",
    "@angular/cli": "^14.2.6",
    "@angular/compiler-cli": "^14.2.7",
    "@schematics/angular": "^14.2.6",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "^39.3.25",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "lint-staged": "^10.5.4",
    "prettier": "2.7.1",
    "typescript": "~4.8.4"
  }
}
