{
  "name": "angular-playground",
  "version": "1.0.0",
  "browserslist": [
    "> 5%"
  ],
  "scripts": {
    "start": "ng serve --hmr --open --port=4202",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint --fix",
    "prettier:write": "prettier --write --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.css,.scss,.html}\"",
    "e2e": "ng e2e --port=4201 --webdriverUpdate=true",
    "predeploy": "ng build --prod --base-href=/angular-playground/ --outputPath=dist/angular-playground",
    "deploy": "gh-pages -d dist/angular-playground/",
    "clear": "rmdir /q /s node_modules",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,css,html}": [
      "npm run prettier:write"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.7",
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/forms": "~11.2.7",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "@types/file-saver": "^2.0.1",
    "@types/leaflet": "^1.5.19",
    "file-saver": "^2.0.5",
    "leaflet": "^1.7.1",
    "ngx-bootstrap": "^6.2.0",
    "ngx-filesaver": "^11.0.0",
    "ngx-mask": "^11.1.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.7",
    "@schematics/angular": "^11.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "protractor": "~7.0.0",
    "simple-git-hooks": "^2.2.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
